@model ControlIDMvc.Models.Recibo
@{
    ViewData["Title"] = "Recibo";
}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>
    <h3>RECIBO INGRESO - EGRESO <small>Fecha(@Model.Fecha.ToString("dd/MM/yyyy"))</small></h3>
    <hr>
    <h4>Recibi / Entrege A: @Model.Nombres</h4>
    <h4>Suma de :@Model.Costo</h4>
</body>

</html>